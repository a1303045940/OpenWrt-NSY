&pinctrl {
	usb {
		vcc5v0_host_en: vcc5v0-host-en {
			rockchip,pins = <0x00 RK_PA6 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		
		vcc5v0_otg_en: vcc5v0-otg-en {
			rockchip,pins = <0x00 RK_PA5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
	regulator-en {
		vcc3v3_lcd0_n_en: vcc3v3-lcd0-n-en {
			rockchip,pins = <0x00 RK_PC7 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		vcc3v3_lcd1_n_en: vcc3v3-lcd1-n-en {
			rockchip,pins = <0x00 RK_PC5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		vcc3v3_vga_en: vcc3v3-vga-en {
			rockchip,pins = <0x00 RK_PD5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		camera_pwr: camera-pwr {
			rockchip,pins = <0x00 RK_PC1 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		vcc3v3_pcie_en: vcc3v3-pcie-en {
			rockchip,pins = <0x00 RK_PD4 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
	sdio-pwrseq {
		wifi_enable_h: wifi-enable-h {
			rockchip,pins = <0x03 RK_PD5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
	pmic {
	
		pmic_int: pmic-intl {
			rockchip,pins = <0x00 RK_PA3 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	
		soc_slppin_gpio: soc-slppin-gpio {
			rockchip,pins = <0x00 RK_PA2 0 &pcfg_output_low>;
		};
	
		soc_slppin_slp:soc-slppin-slp {
			rockchip,pins = <0x00 RK_PA2 1 &pcfg_pull_none>;
		};
	
		soc_slppin_rst {
			rockchip,pins = <0x00 RK_PA2 2 &pcfg_pull_none>;
		};
	};
	pcie {
		pcie30x1_reset_h: pcie30x1-reset-h {
			rockchip,pins = <0 RK_PC3 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		pcie30x2_reset_h: pcie30x2-reset-h {
			rockchip,pins = <0 RK_PC6 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		pcie2x1_reset_h: pcie2x1-reset-h {
			rockchip,pins = <3 RK_PC1 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
	gmac-reset {
		gmac0_reset: gmac0-reset {
